<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.javafxdemo.LoginController">
   <columnConstraints>
      <ColumnConstraints minWidth="-Infinity" />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
   </rowConstraints>
   <children>
      <Label text="账号:" />
      <Label text="密码:" GridPane.rowIndex="1" />
      <TextField fx:id="userName" GridPane.columnIndex="1" />
      <HBox GridPane.columnIndex="1" GridPane.rowIndex="2">
         <children>
            <Button fx:id="btnLogin" mnemonicParsing="false" onAction="#loginButtonClicked" text="Button" />
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </HBox>
      <PasswordField fx:id="passWord" GridPane.columnIndex="1" GridPane.rowIndex="1" />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</GridPane>
